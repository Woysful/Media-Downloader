      "audio format"        : "m4a"
    },
    "instagram" : {
      "yt-dlp parameters"   : "bv+ba/best",
      "postprocessor args"  : "'-c:v copy -c:a aac'"
    },
    "x"         : {
      "yt-dlp parameters"   : "bv+ba/best"
    },
    "bsky"      : {
      "yt-dlp parameters"   : "bv+ba/best",
      "video format"        : "mkv"
    }
  }
}